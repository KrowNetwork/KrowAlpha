query ViewHistory {
	description: "View transaction history"
	statement:
		SELECT org.hyperledger.composer.system.HistorianRecord LIMIT _$lim
}

query GetAvailableJobs {
	description: "Get all available jobs"
	statement:
		SELECT network.krow.assets.Job WHERE (flags == 1)
}

query GetAvailableJobsByTag {
	description: "Get all available jobs by tag"
    statement:
		SELECT network.krow.assets.Job WHERE (tags CONTAINS _$tag)
}
