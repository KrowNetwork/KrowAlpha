query GetAvailableJobs {
	description: "Get all available jobs"
	statement:
		SELECT network.krow.assets.Job WHERE (flags == 1)
}

query GetAllJobs {
description: "Get all available jobs"
statement:
	SELECT network.krow.assets.Job
}

query GetAvailableJobsByTag {
	description: "Get all available jobs by tag"
    statement:
		SELECT network.krow.assets.Job WHERE ((tags CONTAINS _$tag ) AND (flags == 1))
}
